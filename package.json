{
  "name": "material-ui-redux-snackbar",
  "version": "1.0.0",
  "description": "A snackbar for reactjs, using the redux state container and material-ui",
  "browser": "./dist/index.js",
  "main": "./dist/index.js",
  "module": "./src/index.js",
  "jsnext:main": "./src/index.js",
  "author": "klamoree",
  "license": "MIT",
  "dependencies": {
    "eslint": "^4.19.1",
    "material-ui": "^0.20.0",
    "react": "^16.2.0",
    "reactjs": "^1.0.0",
    "redux": "^3.7.2",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13"
  },
  "peerDependencies": {
    "material-ui": "*",
    "react": "*",
    "react-redux": "*",
    "redux": "*"
  },
  "devDependencies": {},
  "scripts": {
    "build": "npm run build:commonjs & npm run build:umd",
