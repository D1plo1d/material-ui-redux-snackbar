    "material snackbar",
    "redux material snackbar"
  ],
  "bugs": {
    "url": "https://github.com/klamoree/material-ui-redux-snackbar/issues"
  },
  "homepage": "https://github.com/klamoree/material-ui-redux-snackbar#readme"
